{
  "name": "quest-app-api",
  "version": "1.0.0",
  "description": "quest api powered by express and mongoose",
  "main": "index.js",
  "engines": {
    "node": "8.12.0",
    "npm": "6.4.1"
  },
  "scripts": {
    "start": "node ./build/index.js",
    "build": "npm run build-babel",
    "dev": "nodemon ./src/index.js --exec babel-node -e js",
    "build-babel": "babel -d ./build ./src -s",
    "clean": "del build && mkdir build"
  },
  "keywords": [
    "quest-api",
    "express",
    "mongoose",
    "pramod",
    "questionnaire"
  ],
  "author": "pramod",
  "license": "MIT",
  "dependencies": {
    "bcrypt": "^3.0.6",
    "express": "^4.17.3",
    "mongoose": "^5.6.2",
    "passport": "^0.4.0",
    "passport-google-oauth20": "^2.0.0",
    "showdown": "^1.9.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "nodemon": "^1.19.1"
  }
}
